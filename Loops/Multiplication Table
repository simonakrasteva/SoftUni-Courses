namespace MultiplicationTable
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int num = int.Parse(Console.ReadLine());
            int result = 1;

            for (int i = 1; i <= 10; i++)
            {
                result = i * num;
                Console.WriteLine($"{num} x {i} = {result}");
            }
        }
    }
}
